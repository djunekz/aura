{
  "name": "@djunekz/aura-terminal",
  "version": "0.7.13",
  "description": "Adaptive Unified Runtime Assistant (mini OS-Layer)",
  "author": "djunekz",
  "license": "MIT",
  "type": "module",
  "main": "./aura.js",
  "exports": {
    ".": "./aura.js"
  },
  "bin": {
    "aura": "./aura.js"
  },
  "files": [
    "aura.js",
    "core/",
    "marketplace/",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "start": "node aura.js",
    "update-plugins": "node core/plugin.js",
