  "repository": {
    "type": "git",
    "url": "https://github.com/djunekz/aura.git"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "blessed": "^0.1.81",
    "blessed-contrib": "^1.0.11",
    "chalk": "^4.1.2",
    "chokidar": "^3.5.3",
    "form-data": "^2.5.5",
    "fs-extra": "^11.1.1",
    "fuzzy": "^0.1.3",
    "inquirer": "^9.2.7",
    "is-online": "^12.0.2",
    "qs": "^6.14.1",
    "tough-cookie": "^4.1.4",
    "xml2js": "^0.5.0"
  }
